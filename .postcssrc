{
  "modules": true,
  "plugins": {
    "postcss-pxtorem": {
      "rootValue": 16,
      "unitPrecision": 5,
      "propList": ["*"],
      "selectorBlackList": ["body", "html"],
      "replace": true,
      "mediaQuery": false,
      "minPixelValue": 0
    },
    "autoprefixer": {
      "browsers": [">1%", "last 4 versions", "Firefox ESR", "not ie < 9"],
      "flexbox": "no-2009"
    }
  }
}
