{
  "parser": "babel-eslint",
  "plugins": [
      "react",
      "babel"
  ],
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "env": {
      "node": true,
      "es6": true
  },
  "globals": {
      "document": true,
      "test": true,
      "expect": true,
      "describe": true,
      "it": true,
      "fetch": true,
      "window": true,
      "jest": true,
      "beforeEach": true
  },
  "rules": {
      "linebreak-style": [0],
      "no-multi-assign": [0],
      "import/prefer-default-export": [0],

      "react/jsx-filename-extension": [0]
  },
  "extends": "airbnb"
}
